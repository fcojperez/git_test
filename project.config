[project]
	description = Repo for testing git and gerrit features
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Francisco Perez (fcojperez)
[access "refs/for/refs/*"]
	read = group user/Francisco Perez (fcojperez)
	push = group user/Francisco Perez (fcojperez)
	label-Code-Review = -2..+2 group user/Francisco Perez (fcojperez)
	label-Verified = -1..+1 group user/Francisco Perez (fcojperez)
	rebase = group user/Francisco Perez (fcojperez)
	removeReviewer = group user/Francisco Perez (fcojperez)
	submit = group user/Francisco Perez (fcojperez)
[access "refs/heads/*"]
	read = group user/Francisco Perez (fcojperez)
	create = group user/Francisco Perez (fcojperez)
	push = +force group user/Francisco Perez (fcojperez)
	pushMerge = group user/Francisco Perez (fcojperez)
[access "refs/tags/*"]
	push = group user/Francisco Perez (fcojperez)
	push = +force group user/Francisco Perez (fcojperez)
